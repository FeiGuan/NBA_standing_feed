<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>NBA_feed</servlet-name>
		<servlet-class>com.fguan.nbaFeed.NBA_feedServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NBA_feed</servlet-name>
		<url-pattern>/nba_feed</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>teams</servlet-name>
		<servlet-class>com.fguan.nbaFeed.TeamsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>teams</servlet-name>
		<url-pattern>/app/teams</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>app</servlet-name>
		<servlet-class>com.fguan.nbaFeed.AppServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>app</servlet-name>
		<url-pattern>/app</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>app/index.html</welcome-file>
	</welcome-file-list>
</web-app>
